  "version": "2.0.0",
  "tasks": [
    {
      "label": "Moddable SDK: Upload to ESP8266",
      "type": "shell",
      "command": "mcconfig -m -p esp",
      "options": {
        "cwd": "${workspaceFolder}/${relativeFileDirname}"
      },
      "problemMatcher": []
    },
    {
      "label": "Moddable SDK: Upload to ESP8266 (debug)",
      "type": "shell",
      "command": "mcconfig -d -m -p esp",
      "options": {
        "cwd": "${workspaceFolder}/${relativeFileDirname}"
      },
      "problemMatcher": []
    },{
      "label": "Moddable SDK: Upload to Moddable One",
      "type": "shell",
      "command": "mcconfig -m -p esp/moddable_one",
      "options": {
        "cwd": "${workspaceFolder}/${relativeFileDirname}"
      },
      "problemMatcher": []
    },
    {
      "label": "Moddable SDK: Upload to Moddable One (debug)",
      "type": "shell",
      "command": "mcconfig -d -m -p esp/moddable_one",
      "options": {
        "cwd": "${workspaceFolder}/${relativeFileDirname}"
      },
      "problemMatcher": []
    }
  ]
}