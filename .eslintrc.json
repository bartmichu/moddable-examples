{
    "env": {
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "System": "readonly",
        "trace": "readonly",
        "debugger": "readonly"
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "no-bitwise": "off",
        "no-unused-vars": "off",
        "no-debugger": "off"
    }
}